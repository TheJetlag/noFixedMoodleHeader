 {
  "name": "MoreHeightInMoodle",
  "version": "1",
  "description": "Makes the moodle header not sticking on you chromes top. So you have more space when browsing a moodle page.",
  "manifest_version": 2,
  "browser_action": {
    "name": "Manipulate DOM",
    "icons": { "16": "icon16.png",
          	   "48": "icon48.png",
          	   "128": "icon128.png" },
    "default_icon": "icon128.png"
  },
  "content_scripts": [ {
    "css": ["customStyles.css"],
    "matches": [ "http://*/*", "https://*/*"]
  }]
}
